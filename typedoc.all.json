{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": ["typedoc-plugin-mdn-links"],
  "out": "./docs",
  "tsconfig": "./tsconfig.base.json",
  "entryPoints": ["./libs/*"],
  "entryPointStrategy": "packages",
  "exclude": ["./libs/pmachine"],
  "readme": "./MODULE.md",
  "markdownItOptions": {
    "html": true,
    "linkify": true,
    "typographer": true
  },
  "name": "SCI.js"
}
